{
  "$schema": "http://json-schema.org/draft-2020-12/schema#",
  "title": "Adacta Reports Shopping Bag",
  "description": "Adacta Reports Shopping Bag project file.",
  "type": "object",
  "properties": {
    "Version": {
      "format": "regex",
      "minLength": 7,
      "maxLength": 11,
      "pattern": "^\\d{1,2}\\.\\d{1,2}\\.\\d{1,2}\\.\\d{1,2}$",
      "type": "string"
    },
    "ProjectName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "ProjectCode": {
      "format": "regex",
      "minLength": 6,
      "maxLength": 7,
      "pattern": "^\\d{2}\\.\\d{3}[a-zA-Z]?$",
      "type": "string"
    }
  },
  "required": [
    "Version",
    "ProjectName",
    "ProjectCode"
  ],
  "additionalProperties": false
}
