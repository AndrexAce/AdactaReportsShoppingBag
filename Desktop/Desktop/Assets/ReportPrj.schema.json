{
  "$schema": "http://json-schema.org/draft-2020-12/schema#",
  "$id": "https://adactainternational.com/schemas/AdactaReportsShoppingBag.json",
  "title": "Adacta Reports Shopping Bag",
  "description": "Adacta Reports Shopping Bag project file.",
  "type": "object",
  "properties": {
    "version": {
      "format": "regex",
      "minLength": 5,
      "maxLength": 8,
      "pattern": "^\\d+\\.\\d+\\.\\d+$",
      "type": "string"
    },
    "projectName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "projectCode": {
      "format": "regex",
      "minLength": 6,
      "maxLength": 7,
      "pattern": "^\\d{2}\\.\\d{3}[a-zA-Z]?$",
      "type": "string"
    }
  },
  "required": [
    "version",
    "projectName",
    "projectCode"
  ],
  "additionalProperties": false
}
